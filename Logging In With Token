//use it ONLY if you forget your password but have token
//i'm not responsible for people stealing tokens and using THIS code to steal accounts

function login(token) {
setInterval(() => {
document.body.appendChild(document.createElement `iframe`).contentWindow.localStorage.token = `"${token}"`
}, 50);
setTimeout(() => {
location.reload();
}, 2500);
}

login('insert token here')

//paste it in console accesible from web client while logging in

//Under the console, you’ll see a text field where you can type things

//Now, you need to replace ‘insert token here’
